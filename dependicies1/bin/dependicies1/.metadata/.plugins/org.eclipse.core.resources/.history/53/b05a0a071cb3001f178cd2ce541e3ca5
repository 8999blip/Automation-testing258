	public static <Student> void main(String[] args) {
		@SuppressWarnings("unused")
		ArrayList<Student> alStudents = new ArrayList<Student>();
		alStudents.add(new Student(23,"pune"));
		ArrayList<Student> alStudent;
		alStudent.add(new student(23,"nagpur"));
		alStudent.add(new Student(23,"mumbai"));
		
